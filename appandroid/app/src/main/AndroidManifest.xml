<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.androidhive.materialtabs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".activity.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CustomViewIconTextTabsActivity"
            android:label="@string/title_activity_custom_view_icon_text_tabs"
            android:parentActivityName=".activity.MainActivity" />
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".activity.PhenixInfra" />
        <activity android:name=".activity.PhenixInfra_2" />

        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.SignupActivity" />


        <activity
            android:name=".activity.IntroActivity"
            android:theme="@style/Theme.Intro" />
        <activity android:name=".activity.Main2Activity"></activity>
    </application>

</manifest>