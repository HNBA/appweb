<div class="row">
    <div class="col-xl-4 text-xs-center">
        <div class="card card-default">
            <div class="card-block" style="-webkit-box-shadow: 0 1px 6px rgba(0,0,0,0.175);box-shadow: 0 1px 6px rgba(0,0,0,0.175);">
                  <div id="container" style="height: 400px; min-width: 300px"></div>
            </div>
        </div>
    </div>
<div class="col-xl-4 text-xs-center">
        <div class="card card-default">
            <div class="card-block" style="-webkit-box-shadow: 0 3px 9px rgba(0,0,0,0.175);box-shadow: 0 3px 9px rgba(0,0,0,0.175);">
                <div id="ramcontainer" style="height: 400px; min-width: 300px"></div>
            </div>
        </div>
    </div>
<div class="col-xl-4 text-xs-center">
    <div class="card mb-4 text-center" style="-webkit-box-shadow: 0 1px 6px rgba(0,0,0,0.175);box-shadow: 0 1px 6px rgba(0,0,0,0.175);">
         Disk Storage
        
             <canvas baseChart style="height:416px;" [data]="pieChartData" [labels]="pieChartLabels" [chartType]="pieChartType" (chartHover)="chartHovered($event)" (chartClick)="chartClicked($event)">
            </canvas>
        
    </div>

<ng-container *ngFor="let chocolate of ssh;">{{chocolate}}
     <li *ngIf="chocolate == 'closed'; then loadingg">{{chocolate}}</li>
</ng-container>
<ng-template #loadingg>
    <push-notification #notification
        closeDelay="10000"
        (load)="notification.show()">
</push-notification>
</ng-template>
</div>
